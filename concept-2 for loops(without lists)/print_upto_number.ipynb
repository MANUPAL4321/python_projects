   "metadata": {},
   "outputs": [],
   "source": [
    "#take any number for print upto n:\n",
    "def func_int(n):\n",
    "#here we run number one by one upto n.    \n",
    "    for i in range(1, n+1):\n",
    "#print one by one number upto n.        \n",
    "        print(i) "
